<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {% block stylesheets %}
            {{ encore_entry_link_tags('back_style') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('back_script') }}
        {% endblock %}
    </head>
    <body>
        <header>
            {% block header %}
                {% include 'front/partials/_navbar.html.twig' %}
            {% endblock %}
        </header>
        <div class="row">
            <div class="col-2">
                {% include 'back/partials/_menu.html.twig' %}
            </div>
            <div class="col-10">
                {% block body %}
                {% endblock %}
            </div>
        </div>

        <footer>
            {% block footer %}

            {% endblock %}
        </footer>
    </body>
</html>
